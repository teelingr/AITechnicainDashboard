{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Server",
        "type": "python",
        "request": "launch",
        "module": "flask",
        "env": {
          "FLASK_APP": "backend/server.py",
          "FLASK_ENV": "development",
          "FLASK_DEBUG": "1"
        },
        "args": [
          "run"
        ],
        "console": "integratedTerminal"
      }
    ]
  }
  